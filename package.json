{
  "name": "babel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build:ts": "tsc -p .",
    "build:babel": "babel src --extensions .ts --out-dir distBabel",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  }
}
